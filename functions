        function g(name){
        document.write("hello "+name)
       }
       let name=prompt("Enter your name")
       g(name)
       

       function add(num1,num2){
        document.write(num1+num2)
       }
       add(3,7)
       document.write("<br>")
       let num1=parseFloat(prompt("enter num1"))
       let num2=parseFloat(prompt("enter num2"))
       add(num1, num2)
       

       sum=function(n1,n2){
        let r=n1+n2
        return r
       }
       console.log(sum(8,9))



