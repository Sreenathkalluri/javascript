    <script>
        for(let i=1;i<10;i+2){
            if(i>5){
                continue
            }
            document.write(i+1)
        }
        // 2 4 6 
        //i = 1,3,5, 7,9
        //if(i>5) = 1,3,5, 7(continue),9(continue)
        //i+1= 2,4,6
        for(let i=2;i<8;i++){
            if(i>5){
                continue
            }
            document.write(i+2)
        }
        //i=2,3,4,5
        //if(i>5)= 2,3,4,5 ,6(continue) ,7(continue) ,8(loop breaks)
        //i+2= 4,5,6,7
        //i++ =3,4,5,6,7,8
    </script>
</body>
