<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        const a='thrinesh'
        for(let i in a){
            console.log(a[i]);
            document.write(a[i]+"<br>");
        }
    </script>
</body>
</html>



<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        for(let i=2;i<=8;i+2){
            if(i>4){
                break;
            }
            document.write(i+1)
        }
        //3,5
        //for i==2 (2>4) is false so i+1 ie; 3
        //for i==4 (4>4) is false so i+1 ie; 5
        //for i==6 (6>4) is true so break ie; loop will stop iterating
    </script>
</body>
</html>




