    <script>
        for(let i=2;i<=9;i+2){
            console.log(i+3);
        }
        // ( i )= 2; 4; 6; 8; 11 (failed 11<=9)
        // (i<=9 ) : 2<=9; 4<=9 ;6<=9 ;8<=9 
        // (console.log(i+3)) = 5; 7; 9; 11;
        // (i+2) = 4 ; 6; 8 ; 11 
        

    </script>
</body>
</html>



<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        const numbers=[1,2,3,4,5];
        let sum=0;
        for(let i=0;i<numbers.length;i++ ){
            sum=sum+numbers[i];
        }
        document.write(sum);
    </script>
</body>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
       let a="chiru";
      for(let i=1;i<=5;i++){
            document.write(a+"<br>");
        }
        document.write("<br>");
        for(let i=1;i<=10;i++){
            document.write(`9x${i}=${9*i}<br>`);
        }
        document.write("<br>");
    </script>
</body>
</html> 










