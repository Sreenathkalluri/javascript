    <script>
        const n=parseInt(prompt("enter number"))
        function prime(n){
        for(let i=1;i<=n/2;i++){
            if(n%i==0){
                return false
            }
        }
        return true
    }
    const a=prime(n)
    if(a){
        document.write(n+" given number is not a prime number");
    }
    document.write(n+" given number is prime")


    const n2=parseInt(prompt("enetr num"))
    




    </script>
